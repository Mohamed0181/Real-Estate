<odoo>
    <!-- Tree View -->
     <record id="owner_tree" model="ir.ui.view">
         <field name="name">Owner Tree</field>
         <field name="model">owner</field>
         <field name="arch" type="xml">
             <list>
                <field name="name"/>
                <field name="phone"/>
                <field name="address"/>
                 <field name="image"/>
             </list>
        </field>
    </record>


    <!-- Form View -->
    <record id="owner_form_view" model="ir.ui.view">
        <field name="name">Owner Form</field>
        <field name="model">owner</field>
        <field name="arch" type="xml">

            <form>
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="phone"/>
                        <field name="address"/>
                        <field name="property_ids" widget="many2many_tags"/>
                        <field name="image"/>

                    </group>
                 </sheet>
            </form>

        </field>
    </record>

    <!--  Action -->
    <record id="owner_action" model="ir.actions.act_window">
        <field name="name">Owner</field>
        <field name="res_model">owner</field>
        <field name="view_mode">list,form</field>
    </record>
    <!-- owner Menu -->
    <menuitem
        id="owner_menu"
        name="Owner"
        parent="configration_menu"
        action="owner_action"
    />
</odoo>