<odoo>
        <record id="property_history_list" model="ir.ui.view">
        <field name="name">Property History</field>
        <field name="model">property_history</field>
        <field name="arch" type="xml">
            <list create="0" edit="0" delete="0">
                <field name="user_id"/>
                <field name="property_id"/>
                <field name="old_state"/>
                <field name="new_state"/>
            </list>
        </field>
    </record>

    <record id="property_history_form" model="ir.ui.view">
        <field name="name">Property History</field>
        <field name="model">property_history</field>
        <field name="arch" type="xml">
            <form create="0" edit="0" delete="0">
                <sheet>

                    <group>
                        <group>
                            <field name="user_id"/>
                            <field name="property_id"/>
                        </group>
                         <group>
                            <field name="old_state"/>
                            <field name="new_state"/>
                             <field name="reason"/>
                        </group>
                    </group>
                    <notebook>
                        <page string="Bedrooms">
                            <field name="lines_ids" readonly="1">
                                <list>
                                    <field name="description"/>
                                    <field name="area"/>
                                </list>
                            </field>
                        </page>
                    </notebook>
                </sheet>
            </form>
        </field>
    </record>

    <record id="property_history_action" model="ir.actions.act_window">
        <field name="name">Property History</field>
        <field name="res_model">property_history</field>
        <field name="view_mode">list,form</field>
    </record>

    <menuitem id="property_history"
              name="Property History"
              parent="properties_menu"
              action="property_history_action"
    />
</odoo>